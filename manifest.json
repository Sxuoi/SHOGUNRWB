{
  "manifest_version": 3,
  "name": "Free Game Notifier",
  "version": "1.0",
  "description": "Dapatkan notifikasi saat ada game gratis di Steam, Epic Games, dll.",
  "permissions": [
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "https://api.freestuffbot.xyz/v2"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "128": "icons/icon.png"
  }
}
